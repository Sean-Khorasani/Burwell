{
  "description": "Find files by extension using atomic operation",
  "parameters": {
    "directory": "Directory to search in",
    "extension": "File extension to search for (e.g., .pdf, .jpg)",
    "store_as": "Variable name to store results (default: foundFiles)"
  },
  "sequence": [
    {
      "command": "UIA_FIND_FILES_BY_EXTENSION",
      "parameters": {
        "directory": "${directory}",
        "extension": "${extension}",
        "store_as": "${store_as}"
      }
    }
  ]
}