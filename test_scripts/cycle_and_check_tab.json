{
  "description": "Move to next tab and check for DeepSeek or return to start",
  "parameters": {},
  "sequence": [
    {
      "comment": "Move to next tab with Ctrl+Tab",
      "command": "EXECUTE_SCRIPT",
      "parameters": {
        "script_path": "test_scripts/uia_send_hotkey.json",
        "variables": {
          "window": "${targetWindow}",
          "modifier": "VK_CONTROL",
          "key": "VK_TAB"
        }
      }
    },
    {
      "command": "WAIT",
      "parameters": {
        "duration": "200ms"
      }
    },
    {
      "comment": "Get current tab title",
      "command": "UIA_GET_WINDOW_TITLE",
      "parameters": {
        "hwnd": "${targetWindow}",
        "store_as": "currentTabTitle"
      }
    },
    {
      "comment": "Check if we're back to starting tab (full cycle completed)",
      "command": "IF_EQUALS",
      "parameters": {
        "variable": "currentTabTitle",
        "value": "${startingFingerprint}",
        "store_as": "backToStartingTab"
      }
    },
    {
      "comment": "If not back to start, check this tab for DeepSeek",
      "command": "IF_EQUALS",
      "parameters": {
        "variable": "backToStartingTab",
        "value": "false",
        "store_as": "shouldCheckTab"
      }
    },
    {
      "comment": "Check current tab for DeepSeek (only if not back to start)",
      "command": "EXECUTE_SCRIPT",
      "parameters": {
        "script_path": "test_scripts/check_if_deepseek_found.json"
      }
    }
  ]
}