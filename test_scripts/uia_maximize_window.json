{
  "description": "Maximize window using Windows keyboard shortcut",
  "parameters": {
    "hwnd": "Window handle to maximize"
  },
  "sequence": [
    {
      "command": "UIA_FOCUS_WINDOW",
      "parameters": {
        "hwnd": "${hwnd}"
      }
    },
    {
      "command": "UIA_KEY_PRESS",
      "parameters": {
        "key": "VK_LWIN"
      }
    },
    {
      "command": "UIA_KEY_PRESS",
      "parameters": {
        "key": "VK_UP"
      }
    },
    {
      "command": "UIA_KEY_RELEASE",
      "parameters": {
        "key": "VK_UP"
      }
    },
    {
      "command": "UIA_KEY_RELEASE",
      "parameters": {
        "key": "VK_LWIN"
      }
    }
  ]
}