{
  "description": "Find windows by title pattern with configurable exact match",
  "parameters": {
    "titlePattern": "Title pattern to search for",
    "exactMatch": "Exact match flag (default: false)",
    "store_as": "Variable name to store results (default: foundWindows)"
  },
  "sequence": [
    {
      "command": "UIA_FIND_WINDOWS_BY_TITLE",
      "parameters": {
        "titlePattern": "${titlePattern}",
        "exactMatch": "${exactMatch}",
        "store_as": "${store_as}"
      }
    }
  ]
}