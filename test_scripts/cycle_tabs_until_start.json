{
  "description": "Cycle through tabs until back to starting tab or DeepSeek found",
  "parameters": {},
  "sequence": [
    {
      "comment": "Get current tab title",
      "command": "UIA_GET_WINDOW_TITLE",
      "parameters": {
        "hwnd": "${targetWindow}",
        "store_as": "currentTabTitle"
      }
    },
    {
      "comment": "Check if back to starting tab",
      "command": "IF_EQUALS",
      "parameters": {
        "variable": "currentTabTitle",
        "value": "${startingTabTitle}",
        "store_as": "backToStart"
      }
    },
    {
      "comment": "Stop cycling if back to starting tab (searched all tabs)",
      "command": "CONDITIONAL_STOP",
      "parameters": {
        "condition_variable": "backToStart"
      }
    },
    {
      "comment": "Check if current tab contains DeepSeek",
      "command": "EXECUTE_SCRIPT",
      "parameters": {
        "script_path": "test_scripts/check_if_deepseek_found.json",
        "result_variable": "currentTabCheck"
      }
    },
    {
      "comment": "Stop if DeepSeek found",
      "command": "CONDITIONAL_STOP",
      "parameters": {
        "condition_variable": "isDeepSeekTrue"
      }
    },
    {
      "comment": "Move to next tab",
      "command": "EXECUTE_SCRIPT",
      "parameters": {
        "script_path": "test_scripts/uia_send_hotkey.json",
        "variables": {
          "window": "${targetWindow}",
          "modifier": "VK_CONTROL",
          "key": "VK_TAB"
        }
      }
    },
    {
      "command": "WAIT",
      "parameters": {
        "duration": "200ms"
      }
    },
    {
      "comment": "Recursively continue cycling",
      "command": "EXECUTE_SCRIPT",
      "parameters": {
        "script_path": "test_scripts/cycle_tabs_until_start.json",
        "variables": {
          "startingTabTitle": "${startingTabTitle}",
          "targetWindow": "${targetWindow}"
        },
        "result_variable": "nextCycleResult"
      }
    }
  ]
}